.nav_box {
  width: 100%;
  height: 140rpx;
  box-sizing: border-box;
}
.nav_box .nav_boxpo {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  align-content: center;
  padding-left: 80rpx;
  padding-right: 80rpx;
  padding-top: 20rpx;
  box-sizing: border-box;
  width: 100%;
  height: 140rpx;
  background: rgba(255, 255, 255, 0.8);
}
.nav_box .nav_boxpo .nav-boxs {
  width: 100rpx;
  height: 130rpx;
  font-size: 24rpx;
  text-align: center;
}
.nav_box .nav_boxpo .nav-boxs .nav-image {
  width: 80rpx;
  height: 80rpx;
}
.nav_box .position {
  position: fixed;
  z-index: 88;
  left: 0;
  top: 0;
}
