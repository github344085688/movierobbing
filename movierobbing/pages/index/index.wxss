/**index.wxss**/
.movie-container {
  display: inline-block;
  flex-direction: row;
  padding-left: 5rpx;
  padding-right: 5rpx;
}
.movie-imgbox {
  width: 200rpx;
}
.movie-img {
  width: 100%;
  display: block;
}
.movie-title {
  display: flex;
  flex-direction: row;
  flex-flow: nowrap;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  padding: 5rpx;
  width: 200rpx;
  color: #fff;
  background: #a60d14;
  text-align: left;
  margin-bottom: 16rpx;
  font-size: 24rpx;
}
.movie-list-container {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content: space-between;
}
.movies-container {
  display: flex;
  flex-direction: row;
  white-space: nowrap;
  justify-content: space-between;
}
.banner_box {
  padding-left: 20rpx;
  padding-right: 20rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
  width: 100%;
}
.banner-img {
  width: 100%;
}
.connects {
  display: flex;
  padding-left: 20rpx;
  padding-right: 20rpx;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: stretch;
  align-content: flex-start;
  margin-bottom: 15rpx;
}
.connects_box {
  margin-top: 15rpx;
  width: 49%;
}
.connects_img {
  width: 100%;
}
.actititle {
  display: flex;
  margin-left: 20rpx;
  margin-right: 20rpx;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: stretch;
  align-content: flex-start;
  padding-top: 20rpx;
  padding-bottom: 0;
}
.actititle .actititle_box {
  width: 25%;
}
.actititle .actititle_box .actititle_img {
  width: 100%;
}
.actititle .actititle_timing {
  width: 45%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  align-content: center;
}
.actititle .actititle_timing .timing {
  border: 1px solid #eee;
  padding: 2px;
  border-radius: 4px;
  font-size: 26rpx;
}
.commoditylist {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: stretch;
  align-content: stretch;
  margin-top: 0;
  margin-right: 25rpx;
  margin-bottom: 0;
  margin-left: 25rpx;
  box-sizing: border-box;
  padding-top: 0rpx;
  padding-right: 0rpx;
  padding-bottom: 15rpx;
  padding-left: 0rpx;
}
.commoditylist .commodityitem {
  box-sizing: border-box;
  width: 48.8%;
  margin-top: 25rpx;
  margin-right: 0;
  margin-bottom: 0;
  margin-left: 0;
  position: relative;
  display: block;
  padding-top: 0rpx;
  padding-right: 0rpx;
  padding-bottom: 6rpx;
  padding-left: 0rpx;
}
.commoditylist .commodityitem .image_box {
  position: relative;
  z-index: 1;
}
.commoditylist .commodityitem .image_box .ttopright {
  width: 0;
  height: 0;
  border-top: 60rpx solid red;
  border-left: 60rpx solid transparent;
  position: absolute;
  top: -3rpx;
  right: 0;
}
.commoditylist .commodityitem .image_box .topright {
  font-size: 24rpx;
  transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -webkit-transform: rotate(45deg);
  -o-transform: rotate(45deg);
  overflow: hidden;
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 0;
  right: 0;
  color: #ffffff;
}
.commoditylist .commodityitem .image_box .movieemg {
  width: 97%;
  display: block;
  margin-top: auto;
  margin-right: auto;
  margin-bottom: auto;
  margin-left: auto;
}
.commoditylist .commodityitem .title_bgs {
  position: absolute;
  z-index: 0;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 96rpx;
  border-top: #e8e8e8 0rpx solid;
  border-right: #e8e8e8 1rpx solid;
  border-bottom: #e8e8e8 1rpx solid;
  border-left: #e8e8e8 1rpx solid;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: stretch;
  align-content: stretch;
  box-sizing: border-box;
  -moz-box-shadow: 0px 0px 1px #8A8A8A;
  -webkit-box-shadow: 0px 0px 1px #8A8A8A;
  box-shadow: 0px 0px 1px #8A8A8A;
}
.commoditylist .commodityitem .title_box {
  position: absolute;
  background: rgba(255, 255, 255, 0.9);
  z-index: 2;
  border-top: #e8e8e8 1rpx solid;
  border-right: #e8e8e8 0rpx solid;
  border-bottom: #e8e8e8 0rpx solid;
  border-left: #e8e8e8 0rpx solid;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: stretch;
  align-content: stretch;
  box-sizing: border-box;
}
.commoditylist .commodityitem .title_box .tit {
  flex: 3;
  font-size: 30rpx;
}
.commoditylist .commodityitem .title_box .tit .title {
  display: block;
  padding-top: 0rpx;
  padding-right: 15rpx;
  padding-bottom: 0rpx;
  padding-left: 15rpx;
}
.commoditylist .commodityitem .title_box .tit .jshao {
  display: block;
  font-size: 25rpx;
  padding-top: 0rpx;
  padding-right: 15rpx;
  padding-bottom: 0rpx;
  padding-left: 15rpx;
}
.commoditylist .commodityitem .title_box .pinf {
  flex: 1;
  ont-size: 30rpx;
  align-self: center;
  text-align: center;
  border-top: #818080 0rpx solid;
  border-right: #818080 0rpx solid;
  border-bottom: #818080 0rpx solid;
  border-left: #818080 1rpx solid;
}
.slide-image {
  width: 100%;
  height: 100%;
}
#ofoMap {
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  z-index: 1;
}
.movies-template {
  margin-bottom: 5rpx;
  margin-top: 30rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}
.nav_box {
  width: 100%;
  height: 140rpx;
  box-sizing: border-box;
}
.nav_box .nav_boxpo {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  align-content: center;
  padding-left: 80rpx;
  padding-right: 80rpx;
  padding-top: 20rpx;
  box-sizing: border-box;
  width: 100%;
  height: 140rpx;
  z-index: 88;
  left: 0;
  top: 0;
  background: rgba(255, 255, 255, 0.8);
}
.nav_box .nav_boxpo .nav-boxs {
  width: 100rpx;
  height: 130rpx;
  font-size: 24rpx;
  text-align: center;
}
.nav_box .nav_boxpo .nav-boxs .nav-image {
  width: 80rpx;
  height: 80rpx;
}
.nav_box .position {
  position: fixed;
  z-index: 88;
  left: 0;
  top: 0;
}
.hr {
  height: 10rpx;
  background: #cccbcb;
}
